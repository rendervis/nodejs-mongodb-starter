{
  "name": "nodejs-starter",
  "version": "1.0.0",
  "description": "A server for chatkit app",
  "main": "app.js",
  "scripts": {
    "start": "node dist/server.js",
    "dev": "pnpm run build:clean && concurrently \"pnpm run watch-compile\" \"pnpm run watch-dev \"",
    "build:clean": "rimraf -rf dist",
    "build:typescript": "tsc",
    "build": "pnpm run build:clean && pnpm build:typescript && pnpm run build:emails",
    "repl": "node --require ts-node/register/transpile-only --experimental-repl-await repl",
    "type-check": "tsc --pretty --noEmit",
    "unused-export-check": "npx ts-unused-exports tsconfig.json --findCompletelyUnusedFiles --allowUnusedTypes --showLineNumber",
    "watch-compile": "swc src -w --out-dir dist",
    "watch-dev": "delay 1 && nodemon --watch \"dist/**/*\" -e js ./dist/server.js"
  },
  "author": "rendervis",
  "license": "MIT",
  "dependencies": {
    "@sentry/node": "^7.47.0",

    "bcryptjs": "^2.4.3",
    "bluebird": "^3.7.2",
    "body-parser": "^1.20.2",
    "clickhouse": "^2.6.0",
    "compression": "^1.7.4",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "cron-parser": "^4.8.1",
    "crypto-js": "^4.1.1",
    "date-fns": "^2.29.3",
    "dotenv": "^16.0.3",
    "errorhandler": "^1.5.1",
    "express": "^4.18.2",
    "express-async-handler": "^1.2.0",
    "express-jwt": "^6.1.2",
    "express-rate-limit": "^6.7.0",
    "express-serve-static-core": "^0.1.1",
    "jsonwebtoken": "^9.0.0",
    "jwks-rsa": "^3.0.1",
    "lodash": "^4.17.21",
    "md5": "^2.3.0",
    "mongodb": "^3.7.3",
    "mongoose": "^5.13.17",
    "openid-client": "^5.4.0",
    "passport": "^0.4.1",
    "passport-jwt": "^4.0.1",
    "pino": "^8.11.0",
    "pino-http": "^8.3.3",
    "uniqid": "^5.4.0",
    "validator": "^10.11.0",
    "zod": "^3.21.4"
  },
  "devDependencies": {
    "@types/express-serve-static-core": "^4.17.33",
    "@swc/cli": "^0.1.62",
    "@swc/core": "^1.3.49",
    "@types/body-parser": "^1.19.2",
    "@types/cors": "^2.8.13",
    "@types/lodash": "^4.14.192",
    "@types/md5": "^2.3.2",
    "@types/node": "^16.18.23",
    "@types/uniqid": "^5.3.2",
    "concurrently": "^7.6.0",
    "delay-cli": "^2.0.0",
    "json-schema-to-zod": "^0.6.3",
    "nodemon": "^1.19.4",
    "npm-run-all": "^4.1.5",
    "pino-pretty": "^9.4.0",
    "prettier": "^2.8.7",
    "rimraf": "^3.0.2",
    "ts-node": "^10.9.1",
    "typescript": "4.7.4"
  },
  "packageManager": "pnpm@7.6.0"
}
